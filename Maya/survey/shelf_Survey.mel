        -command "// Grab selection\nstring $sel[] = `ls -selection`;\nif (size($sel) != 3) {\n    error(\"You must select EXACTLY 3 locators/points for this function\");\n}\n\n// Get some base values\n$arcName = \"threePtArc\";\n$curveName = $sel[0] + \"_arc\";\nstring $layer[] = `listConnections -type \"displayLayer\" $sel[0]`;\nstring $theParent[] = `listRelatives -parent $sel[0]`;\nstring $temp[];\n$pt1 = `xform -ws -q -t $sel[0]`;\n$pt2 = `xform -ws -q -t $sel[1]`;\n$pt3 = `xform -ws -q -t $sel[2]`;\n\n// Make the short arc\n$arc = `createNode makeThreePointCircularArc -n $arcName`;\nsetAttr ($arc + \".pt1\") $pt1[0] $pt1[1] $pt1[2];\nsetAttr ($arc + \".pt2\") $pt2[0] $pt2[1] $pt2[2];\nsetAttr ($arc + \".pt3\") $pt3[0] $pt3[1] $pt3[2];\nsetAttr ($arc + \".d\") 3;\nsetAttr ($arc + \".s\") 8;\n$curve = `createNode nurbsCurve`;\nstring $parent[] = `listRelatives -parent $curve`;\nconnectAttr ($arc + \".oc\") ($curve + \".cr\");\nif ($layer[0] != \"\") {\n\teditDisplayLayerMembers -noRecurse $layer[0] $parent[0];\n}\nif ($theParent[0] != \"\") {\n\t$temp = `parent $curve $theParent[0]`;\n\t$curve = $temp[0];\n}\n$curve = `rename $parent[0] $curveName`;\nselect $curve;\nprint \"\";" 
